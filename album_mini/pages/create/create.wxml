<view class="moblist-box" wx:if="{{showmob&&mobData}}">
    <view class="mob-title">影集模板</view>
    <image catchtap="closeMob" class="mob-back" src="../../images/mob-back.png"></image>
    <view class="mob-tab">
        <view catchtap="changecate" class="tab-item {{cid==0?'tab-active':''}}" data-cid="0">热门</view>
        <view catchtap="changecate" class="tab-item  {{cid==item.id?'tab-active':''}}" data-cid="{{item.id}}" wx:for="{{mobCate}}" wx:key="unique">{{item.title}}</view>
    </view>
    <view class="skin-box">
        <view class="skin-listbox" wx:if="{{mobData}}">
            <view catchtap="chooseThis" class="skin-item-box" data-skinid="{{item.id}}" wx:if="{{cid==0||cid==item.cid}}" wx:for="{{mobData}}" wx:key="unique">
                <image class="skin-item-box-img" src="{{item.thumb}}"></image>
                <image class="skin-item-ishot" src="../../images/newico.png" wx:if="{{item.ishot==1}}"></image>
                <image class="skin-item-ishot" src="../../images/hotico.png" wx:if="{{item.ishot==2}}"></image>
                <view class="maskcooed" wx:if="{{item.id==albumData.skin}}">已选</view>
                <text class="skin-item-title">{{item.title}}</text>
            </view>
        </view>
    </view>
</view>
<view class="moblist-box" wx:if="{{mushow}}">
    <view class="mob-title">选择音乐</view>
    <image catchtap="closeMu" class="mob-back" src="../../images/mob-back.png"></image>
    <view class="mu-list">
        <view class="mu-item" wx:for="{{mulist}}" wx:key="unique">
            <view catchtap="playMusic" class="mu-left" data-music="{{item.music}}">
                <image class="mu-play" src="../../images/mu_stop.png" wx:if="{{selemusic==item.music}}"></image>
                <image class="mu-play" src="../../images/mu_play.png" wx:else></image>
                <text class="mu-name {{selemusic==item.music?'mu-active':''}}">{{item.title}}</text>
            </view>
            <view catchtap="seMusic" class="mu-btn" data-music="{{item.music}}" wx:if="{{selemusic==item.music}}">确定</view>
        </view>
    </view>
</view>
<view class="moblist-box" wx:if="{{picshow}}">
    <view class="mob-title">照片文字</view>
    <image catchtap="closepics" class="mob-back" src="../../images/mob-back.png"></image>
    <view class="photo-edit-box">
        <view class="photo-title-box">
            <text class="photo-title-desc">相册标题</text>
            <input bindblur="blurIput" bindfocus="focusTitle" bindinput="writeTitle" class="photo-title-input" maxlength="40" value="{{inputTitle}}"></input>
        </view>
        <view class="photo-title-len" wx:if="{{titlehide}}">
            <text class="photo-this-font">{{lens}}</text>/40
</view>
        <view class="photo-title-edit">照片编辑</view>
        <view class="pic-list-box" wx:for="{{albumData.imgdata}}" wx:key="unique">
            <view class="pic-list-left">
                <image class="pic-list-img" src="{{item}}"></image>
                <view class="pis-mask">
                    <image catchtap="down" class="pics-down" data-i="{{index}}" src="../../images/down.png" wx:if="{{index+1!=albumData.imgdata.length}}"></image>
                    <image catchtap="up" class="pics-up" data-i="{{index}}" src="../../images/up.png" style="{{index+1==albumData.imgdata.length?'margin-left:220rpx;':''}}" wx:if="{{index!=0}}"></image>
                </view>
            </view>
            <view class="pic-list-right">
                <textarea bindblur="blurtxt" bindinput="writetxt" class="pic-txt-edit" data-ti="{{index}}" maxlength="30" placeholder="点击这里给图片添加文字" value="{{albumData.txtdata[index]}}"></textarea>
                <view class="pic-txt-bottom">
                    <image catchtap="deletePic" class="pic-txt-del" data-i="{{index}}" src="../../images/del.png"></image>
                </view>
            </view>
        </view>
    </view>
    <cover-view catchtap="addpics" class="addpic-btn">+  添加照片</cover-view>
</view>
<block wx:if="{{showphoto&&skinData=='ad'}}">
