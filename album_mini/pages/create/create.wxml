        <button class="{{his?'tool-item-box-his':'tool-item-box'}}" data-edit="1" openType="share">
            <image class="toolimg" src="../../images/pyico.png"></image>
            <text class="tooltxt">好友编辑</text>
        </button>
        <button class="tool-item-sharebox" data-edit="0" openType="share">
