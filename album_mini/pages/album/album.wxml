<image catchtap="playstate" class="music {{music?'musicPlay':''}}" src="../../images/music.png"></image>
<view class="ad-container" style="background:#000" wx:if="{{showphoto&&skinData=='ad'}}">
    <view class="pic-review">
        <view style="width:100vw;height:100vh;display: flex;align-items: center;justify-content: center;">
            <image mode="aspectFill" src="{{img}}" style="height:100%;width:100%;position:absolute;top:0;left:0;filter: blur(60rpx);animation: fadeinAndout 5s linear infinite;"></image>
            <image class="image phad-{{currentAnimation}}" mode="widthFix" src="{{img}}" style="width:100%;"></image>
        </view>
        <view class="showwords">{{text?text:''}}</view>
    </view>
</view>
<image class="songhua" src="../../images/rose.gif" wx:if="{{songhua}}"></image>
<button bindgetuserinfo="createAlbum" class="{{config.albumllads?'editbtn':'editbtnfoot'}}" openType="getUserInfo">
    <image class="edimg" src="{{config.editbtn}}"></image>
</button>
<form bindsubmit="formSubmit" reportSubmit="true">
    <button catchtap="gotoIndex" class="homebtn" formType="submit">
